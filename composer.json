{
    "name": "1001pharmacies/rabbitmq-scheduler",
    "description": "Allow to schedule RabbitMq periodic messages (aka crontab)",
    "type": "application",
    "authors": [
        {
            "name": "Gilles Roustan",
            "email": "gilles@1001pharmacies.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "silex/silex": "^2.0",
        "ivoba/console-service-provider": "^3.0",
        "mtdowling/cron-expression": "^1.1",
        "millerp/rabbitmq-service-provider": "^2.0",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
           }
    }
}
